<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <!-- make form with method=post, action to /postman and add html headers field login_state -->
    <!-- set headers key=login_state, value = true -->
    <!-- submit form -->    
    <!-- write html headers here  -->
    <div id="headers"></div>
    
    <form action="/postman" method="post">
        <input type="text" name="login_state" value="true">
        <input type="submit" value="submit">
    </form>
    <script>
        // get html header keys
        var keys = Object.keys(document.head);
        // get html header values
        var values = Object.values(document.head);
        // get html headers
        var headers = document.head;
        // write headers to div
        document.getElementById("headers").innerHTML = headers;
        // write headers to console
        console.log(headers);
        // write keys to console
        console.log(keys);
        // write values to console
        console.log(values);
        

    </script>
</body>
</html>